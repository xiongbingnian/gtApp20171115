/*积分动画三所需样式*/
.awarded_marks{
    font-size: 1rem;
    color: #f5b908;
    font-weight: 600;
    position: absolute;
    top:1rem;
    right:1rem;
    display: none;
}


/*@media only screen and (min-width:601px) {
    html{
        font-size:130%;
    }
}*/
@media only screen and (max-width:600px) {
    html{
        font-size:100%;
    }
}
@media only screen and (max-width:500px) {
    html{
        font-size:90%;
    }
}
@media only screen and (max-width:400px) {
    html{
        font-size:80%;
    }
}
@media only screen and (max-width:360px){
    html{
        font-size:75%;
    }
}
@media only screen and (max-width:320px){
    html{
        font-size:70%;
    }
}


/*******弹窗样式********/
.popup{
    display: none;
}
.mask{
    max-width:750px;
    min-width:320px;
    height:100%;
    position: fixed;
    top:0;
    right:0;
    left:0;
    bottom: 0;
    margin:auto;
    background: #000;
    opacity: 0.5;
    z-index: 1001;
}
.popup_box{
    position: fixed;
    top:20%;
    right:0;
    left:0;
    margin:auto;
    z-index: 1002;
}
.popup_box>div{
    width:80%;
    background: #fff;
    margin:auto;
    border-radius: 10px;
    padding-bottom: 2%;
}
.popup_header{
    width:100%;
    height: 3rem;
    line-height: 3rem;
    background: #2EE6B5;
    text-align: center;
    border-radius: 10px 10px 0 0;
}
.popup_header>h2{
    color: #fff;
    font-weight: 400;
}
.i_popupClose{
    width: 2.5rem;
    height: 2.5rem;
    background: url(../img/i_popupClose.png) no-repeat center;
    background-size: 70%;
    position: absolute;
    top: 0;
    right: 12%;
    cursor: pointer;
}
.i_popupClose:hover{
    opacity: 0.8;
}
.popup_section{
    width:90%;
    margin:auto;
    color: #999999;
}
.btn_radio{
    display: block;
    width:8rem;
    margin-top: 5%;
    padding:1% 2%;
    border-radius: 5px;
    background: #F8F8F8;
    cursor: pointer;
}
.btn_radio:hover{
    opacity: 0.8;
}
.on_btn_radio{
    background: #EABF50;
    color: #fff;
}
.btn_radio>i{
    display: inline-block;
    width:6px;
    height:6px;
    background: #999999;
    border-radius: 50%;
    margin-right: 5%;
    vertical-align: middle;
}
.popup_title{
    margin: 5% 0 2% 0;
}
.popup_section textarea{
    width:100%;
    height:5rem;
    font-size: 1rem;
    resize: none;
    padding:1%;
    box-sizing: border-box;
}
.popup_hint{
    margin:2rem auto;
    color: #666666;
    font-size: 1.2rem;
    text-align: center;
}
.btn_popup{
    display: block;
    width:50%;
    height:3rem;
    line-height: 3rem;
    text-align: center;
    margin:5% auto;
    background: #13AC83;
    color: #fff;
    border-radius: 10px;
    cursor: pointer;
    font-size: 1.4rem;
}
.btn_popup:hover{
    opacity: 0.8;
}
/************退出登录弹窗*****/
.exit_box{
    background: #EFEFEF;
    position: fixed;
    bottom:0;
    margin:auto;
    left:0;
    right:0;
    z-index: 1002;
}
.btn_logOut,.btn_cancel{
    display: inline-block;
    width:100%;
    height:3rem;
    line-height: 3rem;
    font-size: 1.4rem;
    border-bottom: 1px solid #E4E4E4;
    text-align: center;
    background: #fff;
    cursor: pointer;
}
.btn_logOut{
    margin-bottom: 1%;
    color: #2CC17B;
}
/**********积分规则弹窗**********/
.integrationRule_list{
    margin-top: 5%;
    height: 20rem;
    overflow-y: auto;
}
.integrationRule_list li:first-child{
   font-weight: 600;
}
.integrationRule_list li:last-child{
    border-bottom: none;
}
.integrationRule_list li{
    border-bottom:1px solid #E4E4E4;
    font-size: 0.9rem;
}
.integrationRule_list li>span{
    display: inline-block;
    height:2rem;
    line-height: 2rem;
}
.integrationRule_list li>span:first-child{
    width:43%;
}
.integrationRule_list li>span:nth-child(2){
    width:15%;
}

/************保存成功提示框*************/
.prompt_box{
    position: fixed;
    top: 30%;
    right: 0;
    left: 0;
    margin: auto;
    z-index: 1002;
    display: none;
}
.prompt_box>div{
    width: 60%;
    background: #121212;
    opacity: 0.85;
    color: #FFFFFF;
    text-align: center;
    padding: 2rem 0;
    font-size: 1.5rem;
    border-radius: 10px;
    margin:auto;
}
.prompt_box>div>img{
    width:18%;
    margin-bottom: 2%;
}
/************删除提示弹窗*************/
.del_section{
    text-align: center;
}
.btn_confirm,.btn_return{
    display: inline-block;
    width:7rem;
    height:2rem;
    line-height: 2rem;
    text-align: center;
    border:1px solid #13AC83;
    border-radius: 5px;
    margin:0 3% 3%;
    font-size: 1.2rem;
}
.btn_confirm:hover,.btn_return:hover{
    opacity: 0.8;
    cursor: pointer;
}
.btn_confirm{
    color: #fff;
    background: #13AC83;
}
.btn_return{
    color: #13AC83;
}
/***************图片预览弹窗************/
.imgPvw_popup>.mask{
    opacity: 1;
}
.close_box{
    height:4rem;
    line-height: 4rem;
    position: fixed;
    top:0;
    z-index: 1003;
}
.imgPvw_box{
    height:100%;
    position: fixed;
    top:0;
    z-index: 1002;
    overflow-y: auto;
}
.pvw_img{
    width:100%;
    position: absolute;
    top:0;
    right:0;
    left:0;
    bottom: 0;
    margin:auto;
}


.btn_13AC83{
    border:1px solid #13AC83!important;
    color: #13AC83!important;
    text-shadow: none!important;
}
.btn_13AC83:hover{
    color: #13AC83!important;
    opacity: 0.8;
}


.btn_bg13AC83{
    color: #fff!important;
    background: #13AC83!important;
    text-shadow: none!important;
}
.btn_bg13AC83:hover{
    opacity: 0.8;
    vertical-align: middle;
}




























