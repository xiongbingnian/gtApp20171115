@charset "utf-8";
html{
    min-width: 320px;
}
a {
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    text-decoration: none;
    color: #000;
    cursor: pointer;
}
* { padding:0;margin:0;
    font-family: "microsoft yahei", Helvetica, Arial, sans-serif;
    word-wrap: break-word;
    -webkit-tap-highlight-color: transparent;
}
html ,body {
    margin:0;
    padding:0;
    font-family: "microsoft yahei", Helvetica, Arial, sans-serif;
    /*-webkit-overflow-scrolling: touch;*/
}
input, textarea, select {
    font-family: "microsoft yahei", Helvetica, Arial, sans-serif;
    text-decoration: none;
    outline:none;
    resize: none;
}
h1,h2,h3,h4{
    font-weight: normal;
}
b {
    font-weight: normal;
}
ul,li {
    list-style: none;
}
.left{
    float: left;
}
.right{
    float: right;
}
.hide{
    display: none;
}
.show{
    display: block !important;
}
i{
    display: inline-block;
}
.bg_gray{
    background: #EFEFEF;
}
.container{
    max-width:750px;
    min-width:320px;
    /*min-height:950px;*/
    margin:auto;
    /*position: relative;*/

}
/**********首页头部分***********/

header{
    background: url("../img/bg.png") no-repeat;
    background-size:100% 100%;
    color: #fff;
    position: relative;
    z-index: 1000;
}
.location_box,.queryLocation_box{
    max-width:750px;
    min-width:320px;
    margin:auto;
    text-align: right;
    box-sizing: border-box;
}
.location_box{
    padding-right: 4.5rem;
}
.queryLocation_box{
    padding-right: 2.5rem;
}

.btn_signIn{
    display: inline-block;
    width:4.5rem;
    height:1.8rem;
    line-height: 1.8rem;
    background: #2CC379;
    border-radius:20px 0 0 20px;
    text-align: center;
    position: fixed;
    top:9rem;
    cursor: pointer;
    z-index: 2;
    font-size: 0.8rem;
    color: #fff;
}
.btn_signIn:hover{
    opacity: 0.8;
}
.minute{
    width:2.6rem;
    z-index: 1;
    font-size: 1.5rem;
    color: #f5b908;
    font-weight: 600;
    position: fixed;
    top: 6rem;
}

.head_box{
    width:23%;
    display: inline-block;
    padding: 1rem 0 1.5rem 1rem;
    box-sizing: border-box;
    text-align: center;
}
.img_head{
    width:75%;
    min-width: 4rem;
    max-width: 5.5rem;
    border:8px solid #2DCC8C;
    border-radius: 50%;
}
.btn_exit{
    display: block;
    width: 70%;
    height: 1.6rem;
    line-height: 1.8rem;
    min-width: 5rem;
    max-width: 5.5rem;
    background: #2CC27B;
    color: #fff;
    font-size: 0.8rem;
    margin: 2% auto 0;
    border-radius: 15px;
    -webkit-box-shadow: 2px 2px 3px #52B198;
    -moz-box-shadow: 2px 2px 3px #52B198;
    box-shadow: 2px 2px 3px #52B198;
}
.btn_exit:hover{
    opacity: 0.8;
    cursor: pointer;
}
.info_box{
    width:74%;
    display: inline-block;
    padding:1.5rem 0 2rem 1rem;
    box-sizing: border-box;
}
.student_name{
    display: inline-block;
    height: 1.8rem;
    line-height: 1.8rem;
}
.queryLocation_box>a{
    position: fixed;
    top: 5rem;
    z-index:2;
}
.redDot_query{
    width: 8px;
    height: 8px;
    background: #FF0404;
    border-radius: 50%;
    position: absolute;
    top: 0;
    right:2px;
    z-index: 3;
}
.i_query{
    width: 2.5rem;
    height: 2.5rem;
    background: url(../img/i_query.png) no-repeat center;
    background-size: 90%;
}
.i_query:hover{
    opacity: 0.8;
    cursor: pointer;

}
.integral{
    background: url("../img/bg_integral.png") no-repeat center;
    background-size:100% 100% ;
    padding: 3px 0.5rem 3px 3rem;
}
.integral:hover{
    opacity: 0.8;
    cursor: pointer;
}
.integral span{
    height:1.3rem;
    line-height: 1.3rem;
    color: #fff;
    font-size: 0.8rem;
}
.studentInfo_list>li{
    line-height: 1.8rem;
}
.teacher_name{
    color: #065842;
}
.teacher_name:hover{
    opacity: 0.8;
}
