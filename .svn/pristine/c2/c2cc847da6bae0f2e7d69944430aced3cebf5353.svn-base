//添加指导记录页面跳转
mui(".info_subtitle").on("tap",".guideRecordAdd",function(){
	mui.openWindow({
		url:"guideRecordAdd.html",
		show: {
			aniShow: 'slide-in-right',
			duration: 300
		},
		waiting:{
	      autoShow:true,//自动显示等待框，默认为true
	      title:'正在加载...',//等待对话框上显示的提示内容
	    }
	});
});
