    border-radius: 10px;
}
.btn_operation:hover{
    opacity: 0.8;
    cursor: pointer;
}
.this_week{
    background: #FDEDD1;
}
.this_week td,.this_monthly td{
    color: #666666;
}
.next_week td,.next_monthly td{
    color: #CCCCCC;
}
.monthly_box{
    position: relative;
}
.perpendicular_line{
    width:1px;
    border:none;
