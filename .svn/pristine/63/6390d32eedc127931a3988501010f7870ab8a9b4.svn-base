}
.statistics_num{
    color: #089A73;
    font-size: 1.4rem;
}
.weekly_table,.monthly_table{
    width:100%;
    box-sizing: border-box;
    margin: 2% 0 5%;
    background: #fff;
    border-collapse: collapse;
}
.weekly_table th,.monthly_table th{
    line-height: 3.5rem;
    font-size: 1.2rem;
