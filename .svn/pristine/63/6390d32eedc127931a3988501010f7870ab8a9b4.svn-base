@charset "utf-8";
/*********子页面公共头部样式***********/
.fold_head{
    padding:1rem 1rem 0.5rem;
}
.fold_head>span{
    position: relative;
}
.redDot_tR{
    width: 8px;
    height: 8px;
    background: #FF0404;
    border-radius: 50%;
    position: absolute;
    top: 0;
    right: 1.6rem;
}
.i_return{
    width: 3rem;
    height: 3rem;
    background: url(../img/i_return.png) no-repeat top;
    background-size: 70%;
}
.i_teacher,.i_headQuery,.i_headSignIn{
    width: 3rem;
    height: 3.3rem;
    margin-right: 1rem;
}
.i_teacher{
    background: url("../img/i_teacher.png") no-repeat top;
    background-size:70%;
}
.i_headQuery{
    background: url("../img/i_headQuery.png") no-repeat top;
    background-size:70%;
}
.i_return:hover,.i_teacher:hover,.i_headQuery:hover,.i_headSignIn:hover,.i_pullDown:hover{
    opacity: 0.8;
    cursor: pointer;
}


section{
    /*overflow: hidden;*/
   padding-bottom: 2rem;
}
/*******通知公告*******/
.fCentre{
    width:100%;
    box-sizing: border-box;
    background: #fff;
    padding:0 5% 5%;
    overflow: hidden;
}
.subtitle{
    height:4rem;
    line-height: 4rem;
    font-size: 1.3rem;
    color: #505050;
    position: relative;
}
.informPage_list>li{
    margin-bottom: 5%;
}
.inform_title{
    font-size: 1.1rem;
    color: #808080;
    cursor: pointer;
}
.inform_txt{
    display: inline-block;
    width:80%;
}
.i_unreadMessage{
    width:1.3rem;
    height:1rem;
    margin-right: 2%;
    background: url("../img/i_unreadMessage.png") no-repeat center;
    background-size:100% 100%;
    vertical-align: top;
    position: relative;
    top: 0.3rem;
}
.i_readMessage{
    width:1.3rem;
    height:1.3rem;
    margin-right: 2%;
    background: url("../img/i_readMessage.png") no-repeat center;
    background-size:100% 100%;
    vertical-align: top;
    position: relative;
    top: 0.1rem;
}
.i_foldMessage{
    width:1.3rem;
    height:1rem;
    margin-right: 2%;
    background: url("../img/i_foldMessage.png") no-repeat center;
    background-size:100% 100%;
    vertical-align: top;
    position: relative;
    top: 0.1rem;
}
.right_date{
    font-size: 1rem;
    color: #CCCCCC;
    float: right;
}
.inform_content{
    background: #F0F0F0;
    padding:2% 3%;
    border-radius: 10px;
    margin:2% 1.5rem;
    position: relative;
}
.i_ltAarrows{
    width:0.9rem;
    height:1.2rem;
    background: url("../img/i_ltAarrows.png") no-repeat center;
    background-size:100% 100%;
    position: absolute;
    top: 0;
    left: -0.9rem;
}
.inform_content>p{
    line-height: 1.6rem;
    color: #999999;
}
.inform_paragraph{
    padding-left:2rem;
}


/***********积分详情**********/
.map_box{
    width:100%;
    box-sizing: border-box;
    background: #fff;
}
.map_box>p{
    font-size: 1.3rem;
    color: #505050;
    padding:3% 5% 0;
}
#chart_member{
    width:100%;
    height:17rem;
    margin:auto;
    background: #fff!important;
    /*border-top:1px solid #E4E4E4;*/
    border-bottom:1px solid #E4E4E4;
}
.integral_box{
  margin-top: 2%;
}
.i_explain{
    width: 2.6rem;
    height: 2.6rem;
    background: url(../img/i_explain.png) no-repeat center;
    background-size: 70%;
    position: absolute;
    top: 50%;
    margin-top: -1.3rem;
    right: 0;
}
.i_explain:hover{
    opacity: 0.8;
    cursor: pointer;
}
.integral_title{
    border-top: 1px solid #E4E4E4;
    border-bottom: 1px solid #E4E4E4;
    height:2.8rem;
    line-height: 2.8rem;
    color: #B3B3B3;
    font-size: 1.1rem;
}
.integral_title>span{
    width:31%;
    display: inline-block;
    text-align: center;
}
.total_integral{
    width:100%;
    height:2.8rem;
    line-height: 2.8rem;
    color: #B3B3B3;
    font-size: 1.1rem;
    background: #F8F8F8;
    cursor: pointer;
}
.bg_darkGrey{
    background: #F0F0F0;
}
.total_integral>span,.integral_info li>span{
    width:31%;
    display: inline-block;
    text-align: center;
}
.i_bArrows,.i_tArrows{
    width:0.8rem;
    height:0.6rem;
}
.i_bArrows{
    background: url("../img/i_bArrows.png");
    background-size: 100% 100%;
}
.i_tArrows{
    background: url("../img/i_tArrows.png");
    background-size: 100% 100%;
}
.integral_info{
    position: relative;
}
.i_topArrows{
    width: 2rem;
    height:1rem;
    background: url("../img/i_topArrows.png");
    background-size: 100% 100%;
    position: absolute;
    top:-0.6rem;
    right:16.5%;
}
.integral_info ul{
    background: #C5E793;
    border-radius: 5px;
}
.integral_info li{
    color: #fff;
    height:2rem;
    line-height: 2rem;
    margin:2%;
}

/**********签到************/
#calendar{
    width:100%;
    background: #fff;
    box-sizing: border-box;
}
.singer_r_img{
    display:block;
    width:114px;
    height:52px;
    line-height:45px;
    vertical-align:middle;
    *margin-bottom:-10px;
    text-decoration:none;
}
.singer_r_img:hover{
    background-position:right -53px;
    text-decoration:none;
}
.singer_r_img span{
    margin-left:14px;
    font-size:16px;
    font-weight:700;
}
.singer_r_img.current{
    border:0;
    text-decoration:none;
}
.sign{
    overflow: hidden;
}
.sign table{
    border-collapse: collapse;
    border-spacing: 0;
    width:100%;
}
.sign table tr:nth-child(2){
    margin-top: 1rem;
}
.sign th,.sign td {
    width: 3rem !important;
    height: 4rem !important;
    margin:2%;
    text-align: center;
}
.sign th {
    font-size: 1.2rem;
    line-height: 3rem;
    color: #D1D1D1;
    border-bottom: 1px solid #F0F0F0;
    font-weight: 100;
}
.sign td {
    vertical-align: middle;
    color: #999999;
    font-size: 1.2rem;
    position: relative;
}
.sign .on {
    background-color:#7AD237;
    border-radius: 50%
}
.calendar_month_next,.calendar_month_prev{
    cursor: pointer;
}
.calendar_month_prev,.calendar_month_next {
    width: 3rem;
    height: 3.4rem;
    position: relative;
    top: 1rem;
}
.calendar_month_prev{
    background: url("../img/left_arrows.png") no-repeat center;
    background-size:40% 40%;
}
.calendar_month_next {
    background: url("../img/right_arrows.png") no-repeat center;
    background-size:40% 40%;
}
.calendar_month_span {
    display: inline;
    font-size: 1.5rem;
    color: #0AA97F;
    letter-spacing: 2px;
    margin:0 4%;
}
.sign_succ_calendar_title {
    position: relative;
    text-align: center;
    width:100%;
    line-height: 3rem;
}
.sign_succ_calendar_title>div{
    display: inline-block;
}
.sign_main {
    width: 100%;
    padding:0 5% 5%;
    box-sizing: border-box;
    border-top:1px solid #F0F0F0;
}
.i_yesSignIn{
    width:1rem;
    height:1rem;
    background: url("../img/i_yesSignIn.png") no-repeat center;
    background-size:100% 100% ;
}
.today_noSignIn{
    cursor: pointer;
}
.yesSignIn{
    color: #666!important;
}
.green_circle,.gray_circle,.lightGreen_circle,.green_border{
    width:3.2rem;
    height:3.2rem;
    border-radius: 50%;
    margin:1.2rem auto 0;
}
.green_circle{
    background: #46BD72;
    color: #fff;
}
.gray_circle{
    background: #E6E6E6;
}
.lineHeight{
    line-height: 3rem;
}
/***********日报*************/
.font_green,.font_red,.font_gray{
    display: block;
    font-size: 0.7rem;
}
.font_green{
    color: #0CAB47;
}
.font_red{
    color: #F31111;
}
.font_gray{
    color: #A0A0A0;
}
.lightGreen_circle{
    background: #71E389;
    color: #fff;
    cursor: pointer;
}
.green_border{
    border:1px solid #A1F4DD;
    border-radius: 50%;
    color: #6BEF9C!important;
    box-sizing: border-box;
}
.date{
    display: inline-block;
    margin-top: 0.3rem;
}
.evaluate{
    display: inline-block;
    width:1.6rem;
    height:0.8rem;
    line-height: 0.9rem;
    color: #fff;
    background: #F31111;
    border-radius: 10px;
    font-size: 0.1rem;
    position: absolute;
    top:0.8rem;
    right:0;
}

/**************写日报*************/
.colorGreen,.leave{
    display: inline-block;
    width:4rem;
    color: #10AE83;
    font-size: 1rem;
    position: relative;
}
.leave{
    cursor: pointer;
    text-align: right;
}
.write_txt{
    width:100%;
    height:9.5rem;
    padding:3% 2%;
    box-sizing: border-box;
    border:1px solid #E7E7E7;
    border-radius: 5px;
    font-size: 1rem;
}
.i_notSelected,.i_selected{
    width:1rem;
    height:1rem;
    margin-right: 0.3rem;
    vertical-align: middle;
    position:relative;
    top:-1px;

}
.i_notSelected{
    background: url("../img/i_notSelected.png") no-repeat center;
    background-size:100% 100%;
}
.i_selected{
    background: url("../img/i_selected.png") no-repeat center;
    background-size:100% 100%;
}
.subhead{
    color: #817C73;
    font-size: 1.2rem;
    margin-bottom: 4%;
}
.margin_left{
    margin-left: 5%;
    overflow: hidden;
}
.imgVideo_box{
    width:46%;
    height:10rem;
    border:1px solid #E7E4DC;
    float: left;
    margin:0 2% 2% 0;
    overflow: hidden;
    text-align: center;
    position: relative;

}
.imgVideo_box>img{
    height:100%;
    position: absolute;
    top:0;
    right:0;
    left:0;
    bottom: 0;
    margin:auto;
}
.imgVideo_box>video{
    width:100%;
    height:100%;
    margin:auto;
    background: url("../img/btn_play.png")no-repeat center center;
    background-size: 40% 48%;
}
.del_bg{
    width: 0;
    height: 0;
    border-top: 3rem solid #000;
    border-left: 3rem solid transparent;
    opacity: 0.3;
    position: absolute;
    top:0;
    right:0;
    cursor: pointer;
}
.i_del{
    width:1rem;
    height:1rem;
    background: url("../img/i_del.png") no-repeat center;
    background-size:100% 100%;
    position: absolute;
    top:2%;
    right:2%;
    cursor: pointer;
}
.imgVideoAdd{
    background: url("../img/imgVideoAdd.png") no-repeat center;
    background-size: 40% 60%;
}
.file_upload,.upload{
    border:1px solid #ddd;
    width:100%;
    height:100%;
    opacity: 0;
    cursor: pointer;
}
.btn_save{
    width:80%;
    height:3rem;
    line-height: 3rem;
    text-align: center;
    background: #2CC379;
    margin:1rem auto 3rem;
    display: block;
    clear: left;
    color: #fff;
    border-radius: 5px;
    font-size: 1.2rem;
}
.btn_save:hover,.btn_rating:hover{
    opacity: 0.8;
    cursor: pointer;
}
/************周报 月报************/
.weekly_subtitle{
    width:100%;
    padding: 0 5% 0;
    box-sizing: border-box;
    background: #fff;
}
.weeklyStatistics_list{
    padding:1% 0;
    overflow: hidden;
    border-top:1px solid #ddd;
    border-bottom: 1px solid #ddd;
    background: #fff;
}
.weeklyStatistics_list>li{
    width:32%;
