    border-radius: 50%;
}
.excellent{
    background: #F54949;
}
.good{
    background: #F87F7F;
}
.fair{
    background: #FBB6B6;
}
.poor{
    background: #FDDBDB;
}

.btn_operation{
    display: inline-block;
    font-size: 0.8rem;
    width:4.5rem;
    height:1.7rem;
    line-height: 1.7rem;
