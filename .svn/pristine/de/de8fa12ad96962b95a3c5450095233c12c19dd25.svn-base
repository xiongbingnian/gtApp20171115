@charset "utf-8";
/**********************学生端首页CSS********************/
.remind{
    background: #F3E6D6;
    padding:0.6rem 1rem;
    opacity: 0.8;
}
.remind_content{
    display: inline-block;
    width:80%;
    color: #646464;
    font-size: 1rem;
}
.i_remind{
    width:1.1rem;
    height:1.1rem;
    vertical-align: top;
    position: relative;
    top:1px;
}
.i_close{
	width: 2.5rem;
    height: 2.5rem;
    vertical-align: top;
    position: relative;
    top: -0.8rem;
}
.i_remind{
    margin-right: 1%;
    background: url("../img/i_remind.png") no-repeat center;
    background-size:100% 100% ;
}
.i_close{
    float: right;
    background: url("../img/i_close.png") no-repeat center;
    background-size:50% ;
    cursor: pointer;
}
.i_close:hover{
    opacity: 0.8;
}



section{
    /*padding-bottom: 2rem;*/
}
.module_box{
    background: #fff;
    margin-top: 2%;
    /*border-top: 1px solid #E4E4E4;*/
    /*border-bottom: 1px solid #E4E4E4;*/
}
.module_box>p{
    height:2rem;
    line-height: 2rem;
    padding:1rem;
    border-bottom: 1px solid #E4E4E4;
    position: relative;
}
.module_box>p>a{
    display: inline-block;
    width:100%;
}
.subtitle{
    font-size: 1.3rem;
    color: #505050;
}
.i_allSubtitle{
    width:0.5rem;
    height:0.9rem;
    background: url("../img/i_allSubtitle.png") no-repeat center;
    background-size: 100% 100%;
    position: absolute;
    top:50%;
    right:1rem;
    margin-top: -0.45rem;
}
.i_allSubtitle:hover{
    opacity: 0.8;
}
.inform_box{
    height:10rem;
    overflow: hidden;
}
.inform_list{
    background: #fff;
    padding:0.5rem 1rem;

    height:9rem;
    overflow: hidden;
}
.inform_list li{
    height: 3rem;
    line-height: 3rem;
    position: relative;
    overflow: hidden;
}
.inform_list li:hover{
    background: #F7F7F7;
}
.inform_list li a{
    color: #959595;
}
.red_dot{
    width:6px;
    height:6px;
    border-radius: 50%;
    background: #FF0000;
    margin-right: 1%;
    -webkit-box-shadow: 2px 2px 3px #CCCCCC;
    -moz-box-shadow: 2px 2px 3px #CCCCCC;
    box-shadow: 2px 2px 3px #CCCCCC;
    position: absolute;
    top:48%;
}
.subtitle_title{
    display: inline-block;
    width:95%;
    line-height: 1.6rem;
    margin-left: 3%;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.menu_list{
    background: #fff;
    overflow: hidden;
}
.menu_list li{
    float: left;
    width:25%;
    text-align: center;
    position: relative;
    border-bottom: 1px solid #E4E4E4;
    border-right:1px solid #E4E4E4;
    box-sizing: border-box;
}
.menu_list li>a{
	display: inline-block;
	width: 100%;
	height: 100%;
	padding: 1rem 0;
}
.menu_list li:nth-child(4),.menu_list li:nth-child(8){
    border-right:none;
}
.menu_list li:nth-child(8){
    border-bottom:1px solid #E4E4E4 !important;
}
.menu_list li:last-child{
    border:none;
}
.menu_list li i{
    width:2rem;
    height:1.7rem;
    margin-bottom: 0.5rem;
}
